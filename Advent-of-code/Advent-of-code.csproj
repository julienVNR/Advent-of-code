<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <RootNamespace>Advent_of_code</RootNamespace>
    <LangVersion>preview</LangVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="morelinq" Version="3.3.2" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Resources\Input2021.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Input2021.resx</DependentUpon>
    </Compile>
    <Compile Update="Resources\Input2020.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Input2020.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Resources\Input2021.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Input2021.Designer.cs</LastGenOutput>
    </EmbeddedResource>
    <EmbeddedResource Update="Resources\Input2020.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Input2020.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

</Project>
